{
  "name": "monica-dev.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:dev": "dotenv -e .env.development -- next build",
    "clear:icon": "rm -rf src/ui/icons",
    "dev": "next dev",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,json}'",
    "format:check": "prettier --check ./**/*.{js,jsx,ts,tsx,json}",
    "format:icon": "prettier --write src/ui/icons/*",
    "gen:svgr": "npm run clear:icon && svgr --typescript -d src/ui/icons src/assets/icons && npm run format:icon && npm run lint:icon",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:icon": "eslint --fix src/ui/icons/*",
    "openai:generate-fixture": "dotenv -e .env -- node ./scripts/generate-fixture.js",
    "start": "next start",
    "start:dev": "dotenv -e .env.development -- next start",
    "storybook": "storybook dev -p 6006"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@vanilla-extract/css": "^1.9.5",
    "@vanilla-extract/next-plugin": "^2.1.3",
    "@vercel/analytics": "^1.0.1",
    "clsx": "^1.2.1",
    "encoding": "^0.1.13",
    "graphql-request": "^5.2.0",
    "katex": "^0.16.4",
    "mdast": "^3.0.0",
    "mikanjs": "^1.0.14",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "refractor": "^4.8.1",
    "rehype-katex": "^6.0.3",
    "rehype-parse": "^8.0.4",
    "rehype-prism-plus": "^1.5.1",
    "rehype-react": "^7.2.0",
    "rehype-stringify": "^9.0.3",
    "remark-directive": "^2.0.1",
    "remark-extract-toc": "^1.1.0",
    "remark-flexible-code-titles": "^1.1.2",
    "remark-gfm": "^3.0.1",
    "remark-image-size": "^0.1.3",
    "remark-link-meta": "^0.0.6",
    "remark-math": "^5.1.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-slug": "^7.0.1",
