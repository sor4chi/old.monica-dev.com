{
  "name": "portfolio-neo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:dev": "dotenv -e .env.development -- next build",
    "dev": "next dev",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,json}'",
    "format:check": "prettier --check ./**/*.{js,jsx,ts,tsx,json}",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "openai:generate-fixture": "dotenv -e .env -- node ./scripts/generate-fixture.js",
    "prisma:generate:dev": "dotenv -e .env.development -- prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.development -- prisma migrate dev --name prisma-init",
    "prisma:reset--danger": "dotenv -e .env.development -- prisma db push --accept-data-loss --schema=./prisma/reset.prisma;dotenv -e .env.development -- prisma db push --accept-data-loss",
    "prisma:studio": "dotenv -e .env.development -- prisma studio",
    "start": "next start",
    "start:dev": "dotenv -e .env.development -- next start"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@vanilla-extract/css": "^1.9.5",
    "@vanilla-extract/next-plugin": "^2.1.1",
    "@vercel/og": "^0.4.0",
    "clsx": "^1.2.1",
    "katex": "^0.16.4",
    "mdast": "^3.0.0",
    "mikanjs": "^1.0.14",
    "next": "13.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "refractor": "^4.8.1",
    "rehype-katex": "^6.0.2",
    "rehype-parse": "^8.0.4",
    "rehype-prism-plus": "^1.5.1",
    "rehype-react": "^7.1.2",
    "rehype-stringify": "^9.0.3",
    "remark-directive": "^2.0.1",
    "remark-extract-toc": "^1.1.0",
    "remark-flexible-code-titles": "^1.1.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-slug": "^7.0.1",
    "twemoji": "^14.0.2",
    "typescript": "4.9.5",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/mdast": "^3.0.10",
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.1.0",
    "dotenv-expand": "^10.0.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-redos": "4.5.0-beta.2",
    "eslint-plugin-sort": "^2.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.4",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-sort-json": "^1.0.0",
    "ts-node": "^10.9.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "@vanilla-extract/next-plugin@2.1.1": "patches/@vanilla-extract__next-plugin@2.1.1.patch",
      "next@13.2.3": "patches/next@13.2.3.patch"
    }
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seeds/index.ts"
  }
}
